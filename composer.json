{
  "name": "crealistiques/cre-falclearpagecache",
  "type": "typo3-cms-extension",
  "description": "Automatically clear the cache for pages which include changed/new files by uploads content element.",
  "keywords": [
    "TYPO3",
    "extension",
    "backend"
  ],
  "homepage": "http://crealistiques.de",
  "authors": [
    {
      "name": "Steffen Wargalla",
      "role": "Developer",
      "homepage": "http://crealistiques.de"
    }
  ],
  "license": [
    "GPL-2.0+"
  ],
  "require": {
    "typo3/cms": "8.7.*"
  },
  "autoload": {
    "psr-4": {
      "Crealistiques\\CreFalclearpagecache\\": "Classes"
    }
  },
  "replace": {
    "cre-pagetreewidth": "self.version"
  }
}
